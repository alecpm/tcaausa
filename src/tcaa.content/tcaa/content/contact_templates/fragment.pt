<div class="contactpage"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="tcaa.content"
    tal:define="custom_style context/@@custom-page-style"
    tal:attributes="style custom_style/page_style">

    <div class="fl">
        <div class="location"   tal:content="structure context/locationOne/output"/>
        <div class="zone"       tal:content="structure context/zoneOne/output"/>
    </div>
    <div class="fl">
        <div class="location"   tal:content="structure context/locationTwo/output"/>
        <div class="zone"       tal:content="structure context/zoneTwo/output" />
    </div>
    <div class="fl">
        <div class="location"   tal:content="structure context/locationThree/output"/>
        <div class="zone"       tal:content="structure context/zoneThree/output" />
    </div>
    <div class="fl social">
        <h3>Follow Us</h3>
        <p>
            <a href="#">Facebook</a><br/>
            <a href="#">Twitter</a><br/>
            <a href="#">RSS</a><br/>
            <a href="#">Vimeo</a>
        </p>
    </div>
</div>
